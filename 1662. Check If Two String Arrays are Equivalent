class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        string str1,str2;
        for(int i=0,j=0;i<word1.size(),j<word2.size();i++,j++)
        {
            str1+=word1[i];
            str2+=word2[j];
        }

        if(str1==str2)
        {
            return 1;
        }
        return 0;
        
    }
};
